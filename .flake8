[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    migrations,
    staticfiles,
    media,
    logs,
    .pytest_cache,
    .coverage,
    htmlcov
per-file-ignores =
    __init__.py:F401
    migrations/*:E501
max-complexity = 10
select = C,E,F,W,B,B950
